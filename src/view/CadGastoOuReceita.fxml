<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.CadGastoOuReceitaController">
   <children>
      <Label layoutX="117.0" layoutY="72.0" text="CADASTRE UM RECEBIMENTO OU UM GASTO">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <CheckBox fx:id="chkRecebimento" layoutX="220.0" layoutY="169.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="161.0" text="Recebimento" textFill="#17f210">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="chkGasto" layoutX="220.0" layoutY="237.0" mnemonicParsing="false" prefWidth="124.0" text="Gasto" textFill="#f05e11">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </CheckBox>
      <Button fx:id="btOk" layoutX="263.0" layoutY="317.0" mnemonicParsing="false" onAction="#onBtOk" text="OK">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
